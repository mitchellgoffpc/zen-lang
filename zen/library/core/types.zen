;; zen/types is the very first module in the Zen Prelude. It defines some really
;; essential classes and functions that all of the other modules in the Prelude
;; require.

(class Object)
(class Bool)
(class Int)
(class String)

(class Symbol)
(class Keyword)
(class Tuple)
(class Array)
(class Function)

(def bool (x) (x :bool))
(def int (x) (x :int))
(def str (x) (x :str))
